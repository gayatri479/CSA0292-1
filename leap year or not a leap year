#include <stdio.h>

int main() 
{
int year=2024,month=10,date=03;
if(year%400==0)
{
    printf("%d/%d/%d is a leap year",date,month,year);
}
else if(year%400==0)
{
    printf("%d/%d/%d is not a leap year",date,month,year);
}
else if(year%4==0)
{
    printf("%d/%d/%d is a leap year",date,month,year);
}
else
{
    printf("%d/%d/%d is not a leap year",date,month,year);
}
}
