#include <stdio.h>

int main() 
{
int num=121,originalnum,reversed=0,rem;
originalnum=num;
while(originalnum!=0)
{
    rem=num%10;
    reversed=reversed*10+rem;
    num/=10;
}
if(originalnum==reversed)
{
    printf("%d is a palindrome",originalnum);
}
else
printf("%d is not a palindrome",originalnum);

}
