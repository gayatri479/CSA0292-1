#include <stdio.h>

int main() {
  int i,j,a[3][3];
  printf("enter rows and columns\n");
  for(i=0;i<3;++i)
  {
      for(j=0;j<3;j++)
      {
      scanf("%d",&a[i][j]);
      }
  }
  printf("matrix value are\n");
  for(i=0;i<3;i++)
  {
      for(j=0;j<3;j++)
      {
          printf("%d\t",a[i][j]);
      }
      printf("\n");
  }
     return 0;
}
